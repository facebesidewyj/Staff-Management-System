<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE hibernate-mapping PUBLIC 
	    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pers.yijin.bms.domain.Station" table="station">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="stationName"></property>
		<property name="stationOrder"></property>
		<set name="busList" table="bus_station">
			<key column="stationId"></key>
			<many-to-many class="pers.yijin.bms.domain.Bus" column="busId"></many-to-many>
		</set>
	</class>
</hibernate-mapping>