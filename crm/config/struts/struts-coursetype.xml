<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!-- 配置课程包 -->
	<package name="coursetype" extends="common" namespace="/">
		<action name="coursetypeAction_*" class="pers.yijin.crm.coursetype.web.action.CoursetypeAction" method="{1}">
			<result name="findCourseByCondition">/WEB-INF/pages/coursetype/listCourse.jsp</result>
			<result name="addOrEditUI">/WEB-INF/pages/coursetype/addOrEditCourse.jsp</result>
			<result name="addOrEdit" type="redirectAction">coursetypeAction_findCourseByCondition</result>
		</action>
	</package>
</struts>